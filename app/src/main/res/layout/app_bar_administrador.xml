<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Administrador">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#091B2D"
        android:theme="@style/Theme.SivarTravel"
        app:itemTextColor="@color/white">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbara"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:theme="@style/ThemeOverlay.MaterialComponents.Dark"
            app:popupTheme="@style/Theme.SivarTravel.AppBarOverlay"
            app:subtitleTextColor="@color/white"
            app:titleTextColor="@color/white" />

    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_administrador" />



</androidx.coordinatorlayout.widget.CoordinatorLayout>